{
  "name": "redux-action-chain",
  "version": "1.0.0",
  "description": "Higher-order reducer which makes possible dispatching an array of actions at once.",
  "main": "dist/redux-action-chain.cjs.js",
  "module": "dist/redux-action-chain.esm.js",
  "files": [
    "dist",
    "typings"
  ],
  "types": "./typings/index.d.ts",
  "scripts": {
    "build": "run-p build:*",
    "build:bundles": "rollup --config rollup.config.js",
    "build:typings": "tsc --declaration --emitDeclarationOnly --noEmitOnError --declarationDir typings",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,css,md,yml}\"",
    "test:typecheck": "tsc --noEmit",
    "test:sources": "jest --coverage",
    "test:sources:watch": "jest --watch",
    "test": "run-p test:*",
    "coverage": "cat coverage/*/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhovhannes/redux-action-chain.git"
  },
  "keywords": [
    "redux",
    "higher-order reducer",
    "chain",
    "actions"
  ],
  "author": "Hovhannes Babayan <bhovhannes@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhovhannes/redux-action-chain/issues"
  },
  "homepage": "https://github.com/bhovhannes/redux-action-chain#readme",
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@types/jest": "24.0.11",
    "codecov": "3.2.0",
    "husky": "1.3.1",
    "jest": "24.5.0",
    "lint-staged": "8.1.5",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "redux": "4.0.1",
    "rollup": "1.7.4",
    "rollup-plugin-typescript": "1.0.1",
    "ts-jest": "24.0.1",
    "typescript": "3.4.1"
  },
  "peerDependencies": {
    "redux": "^4.0.1"
  }
}
